CREATE TABLE `sd_stat`.`sd_ass_equipment_income` (
  `id` bigint(20) unsigned NOT NULL AUTO_INCREMENT,
  `work_order_no` varchar(50) NOT NULL COMMENT '工单编号',
  `abnormal_slot_income` decimal(16,2) NOT NULL DEFAULT '0.00' COMMENT '异常槽位价值',
  `add_battery_income` decimal(16,2) NOT NULL DEFAULT '0.00' COMMENT '补宝价值',
  `asset_recovery_income` decimal(16,2) NOT NULL DEFAULT '0.00' COMMENT '资产找回价值',
  `recycle_battery_income` decimal(16,2) NOT NULL DEFAULT '0.00' COMMENT '回收宝价值',
  `disconnected_income` decimal(16,2) NOT NULL DEFAULT '0.00' COMMENT '断线价值',
  `plummet_income` decimal(16,2) NOT NULL DEFAULT '0.00' COMMENT '骤降价值',
  `low_moving_pin_income` decimal(16,2) NOT NULL DEFAULT '0.00' COMMENT '低动销价值',
  `abnormal_slot_status` tinyint(5) NOT NULL DEFAULT '1' COMMENT '异常槽位价值计算状态 (1：计算中 2：已完成)',
  `add_battery_status` tinyint(5) NOT NULL DEFAULT '1' COMMENT '补宝价值计算状态 (1：计算中 2：已完成)',
  `asset_recovery_status` tinyint(5) NOT NULL DEFAULT '1' COMMENT '资产找回价值计算状态 (1：计算中 2：已完成)',
  `recycle_battery_status` tinyint(5) NOT NULL DEFAULT '1' COMMENT '回收宝价值计算状态 (1：计算中 2：已完成)',
  `disconnected_status` tinyint(5) NOT NULL DEFAULT '1' COMMENT '断线价值计算状态 (1：计算中 2：已完成)',
  `plummet_status` tinyint(5) NOT NULL DEFAULT '1' COMMENT '骤降价值计算状态 (1：计算中 2：已完成)',
  `low_moving_pin_status` tinyint(5) NOT NULL DEFAULT '1' COMMENT '低动销价值计算状态 (1：计算中 2：已完成)',
  `calculation_date` bigint(11) NOT NULL DEFAULT '0' COMMENT '计算日期（时间戳）',
  `create_time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `create_time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  UNIQUE KEY `uk` (`work_order_no`) USING BTREE
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COMMENT='运维设备价值表';



1、大数据cloudera manager生产集群以及测试集群搭建并维护集群稳定运行
2、mysql业务库离线数据抽取以及实时数据同步到大数据平台
3、mongoDB数据同步大数据平台
4、历史埋点数据同步到新数据平台
5、设备日志heartbeat,access,other数据同步打新数据平台
6、新神策埋点，C端小程序埋点文档整理以及对接前端，神策埋点数据测试，校验，修改以及采集入库
7、帆软