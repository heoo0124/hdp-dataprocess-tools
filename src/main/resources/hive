CREATE TABLE IF NOT EXISTS backup.general_press (   distinct_id STRING ,   time BIGINT ,   event STRING ,   type STRING ,   track_id BIGINT ,   lib STRING ,   lib_method STRING ,   lib_version STRING ,   app_id STRING ,   app_version STRING ,   device_id STRING ,   model STRING ,   carrier STRING ,   network_type STRING ,   wifi BOOLEAN ,   is_first_day BOOLEAN ,   is_first_time BOOLEAN ,   screen_height INT ,   screen_width INT ,   manufacturer STRING ,   latest_scene STRING ,   timezone_offset INT ,   os STRING ,   os_version STRING ,   remote_ip STRING ,   is_login_id BOOLEAN ,   country STRING ,   province STRING ,   city STRING ,   longitude STRING ,   latitude STRING ,   browser STRING ,   browser_version STRING ,   user_id STRING ,   level_id STRING ,   app_name STRING ,   element_id STRING ,   element_type STRING ,   element_name STRING ,   page_name STRING ,   source STRING ,   anonymous_id STRING ,   info STRING   ) row format delimited fields terminated by '\001' location "/data/backup/general_press";

CREATE TABLE IF NOT EXISTS backup.general_view (   distinct_id STRING ,   time BIGINT ,   event STRING ,   type STRING ,   track_id BIGINT ,   lib STRING ,   lib_method STRING ,   lib_version STRING ,   app_id STRING ,   app_version STRING ,   device_id STRING ,   model STRING ,   carrier STRING ,   network_type STRING ,   wifi BOOLEAN ,   is_first_day BOOLEAN ,   is_first_time BOOLEAN ,   screen_height INT ,   screen_width INT ,   manufacturer STRING ,   latest_scene STRING ,   timezone_offset INT ,   os STRING ,   os_version STRING ,   remote_ip STRING ,   is_login_id BOOLEAN ,   country STRING ,   province STRING ,   city STRING ,   longitude STRING ,   latitude STRING ,   browser STRING ,   browser_version STRING ,   user_id STRING ,   level_id STRING ,   app_name STRING ,   element_id STRING ,   element_type STRING ,   element_name STRING ,   page_name STRING ,   source STRING ,   anonymous_id STRING ,   info STRING   ) row format delimited fields terminated by '\001' location "/data/backup/general_view";

CREATE TABLE IF NOT EXISTS backup.sd_ad_visit_record_prod (   index STRING ,   id STRING ,   ad_id INT ,   equip_out_number STRING ,   user_id STRING ,   event_type STRING ,   province_id STRING ,   city_id STRING ,   area_id STRING ,   user_ip STRING ,   source INT ,   platform STRING ,   page_type INT ,   page_show INT ,   page_style INT ,   ad_tag INT ,   shopid STRING ,   longitude STRING ,   latitude STRING ,   ad_visit_count INT ,   ad_click_count INT ,   ad_user_visit_count INT ,   ad_user_click_count INT ,   year STRING ,   month STRING ,   day STRING ,   minute STRING ,   created_at BIGINT   ) row format delimited fields terminated by '\001' location "/data/backup/sd_ad_visit_record_prod";
